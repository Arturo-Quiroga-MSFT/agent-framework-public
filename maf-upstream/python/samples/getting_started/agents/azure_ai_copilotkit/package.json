{
  "scripts": {
    "install:agent": "cd agent && pip install -e .",
    "install:ui": "cd ui && npm install",
    "install": "npm run install:agent && npm run install:ui",
    "dev:agent": "cd agent && python src/main.py",
    "dev:ui": "cd ui && npm run dev",
    "dev": "concurrently \"npm run dev:agent\" \"npm run dev:ui\"",
    "start": "npm run dev"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
